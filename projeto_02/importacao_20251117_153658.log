    SELECT COUNT(*) INTO v_emprestimos_ativos
    FROM Emprestimos
    WHERE id_usuario = p_id_usuario
    AND status_emprestimo = 'Ativo';

    SELECT tu.max_emprestimos, tu.prazo_dias
